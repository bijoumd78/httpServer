{
  "redisHost": "localhost",
  "redisPort": 6379,
  "channelType": [
    {
      "type": "console",
      "loggingLevel": "information",
      "times": "local"
    },
    {
      "type": "file",
      "loggingLevel": "trace",
      "path": "test/log.txt",
      "rotation": "2 K",
      "archive": "timestamp",
      "pattern": "%Y-%m-%d %H:%M:%S %s %t",
      "times": "local",
      "compress": "false",
      "purgeAge": "12 months",
      "purgeCount": "100"
    },
	{
      "type": "database",
      "loggingLevel": "information",
      "name": "Dev",
      "user": "postgres",
      "password": "password",
      "host": "127.0.0.1",
      "port": "5432",
      "tableName": "tests"
    }
  ]
}